{
  "name": "openchs-android",
  "version": "0.0.1",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenCHS/openchs-client.git"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "<rootDir>/test/**/*Test.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|OpenCHSClient)/"
    ],
    "haste": {
      "defaultPlatform": "android",
      "platforms": [
        "android",
        "ios"
      ],
      "providesModuleNodeModules": [
        "react",
        "react-native"
      ]
    }
  },
  "license": "AGPL-3.0",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "test": "jest",
    "coverage": "istanbul cover --report lcov _mocha --require ./node_modules/react-native-mock/mock.js --require src/test/testHelper.js $(find ./test -name '*Test.js')"
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "color": "2.0.0",
    "immutable": "3.8.1",
    "invariant": "2.2.2",
    "lodash": "^4.14.0",
    "moment": "2.18.1",
    "native-base": "^2.3.9",
    "openchs-health-modules": "0.0.1",
    "openchs-models": "0.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-native": "^0.53.0",
    "react-native-aws-cognito-js": "0.0.7",
    "react-native-config": "0.10.0",
    "react-native-deprecated-custom-components": "^0.1.1",
    "react-native-fabric": "0.5.1",
    "react-native-i18n": "0.1.1",
    "react-native-svg": "^6.2.0",
    "react-native-vector-icons": "^4.5.0",
    "realm": "^2.0.12",
    "redux": "3.6.0",
    "stacktrace-js": "2.0.0",
    "transducers-js": "0.4.174",
    "victory-native": "^0.17.1"
  },
  "devDependencies": {
    "babel-jest": "20.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-native": "1.9.2",
    "babel-preset-react-native-stage-0": "1.0.1",
    "chai": "4.1.2",
    "enzyme": "2.4.1",
    "faker": "^4.1.0",
    "jest": "20.0.1",
    "jest-cli": "^21.2.1",
    "react-addons-test-utils": "15.4.1",
    "react-dom": "15.4.2",
    "react-test-renderer": "15.4.2"
  }
}
